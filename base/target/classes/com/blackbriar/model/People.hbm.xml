<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.blackbriar.model">
   <class name="People" table="People">
     <id name="id" column="f_id" type="java.lang.Integer">
       <generator class="native"></generator>
     </id>
     <property name="name" column="f_name" type="java.lang.String"></property>
     <property name="age" column="f_age" type="java.lang.Integer"></property>
   </class>
</hibernate-mapping>